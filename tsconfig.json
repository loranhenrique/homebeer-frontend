{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "types": ["node"],
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "lib": ["es2018", "dom"],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "removeComments": true,
    "resolveJsonModule": true
  }
}
